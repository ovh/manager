{
  "name": "@ovh-ux/manager-documentation",
  "version": "0.2.0",
  "private": true,
  "description": "Manager documentation deployed with GitHub Pages",
  "scripts": {
    "build": "vuepress build docs",
    "postbuild": "yarn build:msc-storybook",
    "build:msc-storybook": "rm -rf ./docs/.vuepress/public/super-components && cd ../packages/super-components && yarn build:storybook && mv ./storybook/dist ../../docs/docs/.vuepress/public/super-components",
    "deploy": "yarn build && gh-pages -d docs/.vuepress/dist -m \"docs: update documentation [skip ci]\"",
    "predev": "yarn build:msc-storybook",
    "dev": "vuepress dev docs"
  },
  "devDependencies": {
    "@vuepress/plugin-search": "2.0.0-beta.53",
    "get-monorepo-packages": "^1.2.0",
    "gh-pages": "^2.2.0",
    "vuepress-webpack": "2.0.0-beta.53"
  }
}
