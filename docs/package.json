{
  "name": "@ovh-ux/manager-documentation",
  "version": "0.2.0",
  "private": true,
  "description": "Manager documentation deployed with GitHub Pages",
  "scripts": {
    "build": "vuepress build docs",
    "prebuild:msc-storybook": "rm -rf ./docs/.vuepress/public/super-components",
    "build:msc-storybook": "yarn workspace @ovhcloud/msc-lib build:storybook",
    "postbuild:msc-storybook": "mv ../packages/super-components/storybook/dist ./docs/.vuepress/public/super-components",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d docs/.vuepress/dist -m \"docs: update documentation [skip ci]\"",
    "predev": "yarn build:msc-storybook",
    "dev": "vuepress dev docs",
    "postbuild:disable": "yarn build:msc-storybook"
  },
  "devDependencies": {
    "@vuepress/plugin-search": "2.0.0-beta.53",
    "get-monorepo-packages": "^1.2.0",
    "gh-pages": "^2.2.0",
    "vuepress-webpack": "2.0.0-beta.53"
  }
}
