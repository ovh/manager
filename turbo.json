{
  "$schema": "https://turbo.build/schema.json",
  "daemon": false,
  "cacheDir": "node_modules/.cache/turbo",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "types/**"],
      "outputLogs": "new-only",
      "env": ["VERSION"]
    },
    "test": {},
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**", "types/**"]
    },
    "start": {
      "persistent": true,
      "cache": false,
      "env": [
        "CONTAINER",
        "LABEU",
        "LABEU_HOST",
        "local2API",
        "REGION",
        "CDS_VERSION",
        "VITE_CONTAINER_APP"
      ],
      "dependsOn": ["^dev"]
    }
  }
}
