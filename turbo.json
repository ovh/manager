{
  "$schema": "https://turbo.build/schema.json",
  "daemon": false,
  "cacheDir": "node_modules/.cache/turbo",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "types/**"],
      "outputLogs": "new-only",
      "env": ["VERSION"]
    },
    "test": {
      "dependsOn": [
        "^test",
        "@ovh-ux/manager-static-analysis-kit#build"
      ],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^dev"],
      "outputs": ["dist/**", "types/**"]
    },
    "start": {
      "persistent": true,
      "cache": false,
      "env": [
        "CONTAINER",
        "LABEU",
        "LABEU_HOST",
        "local2API",
        "REGION",
        "VERSION",
        "VITE_CONTAINER_APP"
      ],
      "dependsOn": ["^dev"]
    },
    "lint:modern": {
      "outputs": [],
      "dependsOn": [
        "^lint:modern",
        "@ovh-ux/manager-static-analysis-kit#build"
      ]
    },
    "lint:modern:fix": {
      "outputs": [],
      "dependsOn": [
        "^lint:modern:fix",
        "@ovh-ux/manager-static-analysis-kit#build"
      ]
    }
  }
}
