  "scripts": {
    "build": "yarn pm:build:ci",
    "build:turbo": "node -e \"console.warn('\\n[DEPRECATED] build:turbo is deprecated. Use: yarn pm:build:ci or yarn build\\n')\" && yarn pm:build:ci",
    "ci:install": "yarn install --non-interactive --prefer-offline",
    "clean": "yarn run clean:dist && rimraf node_modules",
    "clean:deep": "node ./packages/manager-tools/manager-pm/src/manager-pm-deep-clean.js",
    "clean:dist": "rimraf \"packages/**/dist\"",
    "commit": "node scripts/commit.js",
    "generate:module": "yarn manager-forge-module",
    "generate:uapp": "yarn manager-forge-application",
    "generate:uapp:api": "yarn manager-forge-api",
    "generate:uapp:component": "yarn manager-forge-component",
    "generate:uapp:hook": "yarn manager-forge-hook",
    "generate:uapp:page": "yarn manager-forge-page",
    "preinstall": "node ./packages/manager-tools/manager-pm/src/manager-pm-preinstall.js",
    "postinstall": "node ./packages/manager-tools/manager-pm/src/manager-pm-postinstall.js",
    "lint": "yarn pm:lint:ci",
    "lint:staged": "node scripts/lint-staged/lint-staged.mjs",
    "list-dependents": "node scripts/list-dependents.js",
    "manager-cli": "yarn manager-pm --action cli",
    "manager-perf-budgets": "yarn manager-pm --type pnpm --action perfBudgets",
    "packages:publish": "manager-pm --type pnpm --action publish",
    "pm:add:app": "node ./packages/manager-tools/manager-pm/src/manager-pm-add-app.js",
    "pm:add:module": "node ./packages/manager-tools/manager-pm/src/manager-pm-add-module.js",
    "pm:build:ci": "manager-pm --type pnpm --action buildCI",
    "pm:build:full": "manager-pm --type pnpm --action full-build",
    "pm:isolated:install": "node packages/manager-tools/manager-pm/src/manager-pm-isolated-install.js",
    "pm:lint:ci": "manager-pm --type pnpm --action lintCI",
    "pm:prepare:legacy:workspace": "node ./packages/manager-tools/manager-pm/src/manager-pm-prepare-workspace.js",
    "pm:remove:app": "node ./packages/manager-tools/manager-pm/src/manager-pm-remove-app.js",
    "pm:remove:legacy:workspace": "node ./packages/manager-tools/manager-pm/src/manager-pm-remove-workspace.js",
    "pm:remove:module": "node ./packages/manager-tools/manager-pm/src/manager-pm-remove-module.js",
    "pm:start": "manager-pm --type pnpm --action start",
    "pm:test:ci": "manager-pm --type pnpm --action testCI",
    "pm:test:full": "manager-pm --type pnpm --action full-test",
    "prepare": "husky install",
    "release": "manager-pm --type pnpm --action release",
    "start": "yarn pm:start",
    "static-dynamic-quality-check-reports": "yarn manager-pm --type pnpm --action staticDynamicReports",
    "static-dynamic-quality-check-tests": "yarn manager-pm --type pnpm --action staticDynamicTests",
    "test": "yarn pm:test:ci",
    "test:scripts": "make test"
  },
