{
  "name": "@ovh-ux/ng-ovh-contracts",
  "version": "4.0.0",
  "description": "Component to correctly show contracts in OVHcloud control panels",
  "keywords": [
    "angularjs",
    "contracts",
    "ovh",
    "ovhcloud"
  ],
  "homepage": "https://github.com/ovh/manager/tree/master/packages/components/ng-ovh-contracts#readme",
  "bugs": {
    "url": "https://github.com/ovh/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/components/ng-ovh-contracts"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ovh-contracts.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ng-ovh-contracts' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ovh-contracts' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ovh-contracts' --include-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "animated-scroll-to": "^1.2.2"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  },
  "peerDependencies": {
    "@ovh-ux/ui-kit": "^4.1.12",
    "angular": "^1.6.10",
    "angular-translate": "^2.17.0",
    "angular-ui-bootstrap": "~1.3.3",
    "bootstrap": "^4.0.0",
    "jquery": "^2.1.3"
  }
}