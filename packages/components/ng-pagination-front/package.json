  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-pagination-front.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "lint": "run-p lint:css lint:html lint:js lint:md",
    "lint:css": "stylelint 'src/**/*.less' --fix",
    "lint:html": "htmlhint 'src/**/*.html'",
    "lint:js": "eslint --quiet --fix --format=pretty src",
    "lint:md": "eslint --quiet --fix --format=pretty --ext .md .",
    "prepare": "rollup -c",
    "test": "yarn run lint",
    "version": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^7.1.2",
    "@ovh-ux/component-rollup-config": "7.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-formatter-pretty": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-markdown": "^1.0.0",
    "htmlhint": "^0.10.0",
    "husky": "^1.1.0",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "ovh-ui-kit": "^2.24.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "peerDependencies": {
    "angular": "^1.7.5",
    "angular-ui-bootstrap": "~1.3.3",
    "ovh-ui-kit-bs": "^2.2.0"
  }
