{
  "name": "@ovh-ux/ng-pagination-front",
  "version": "8.0.0-alpha.0",
  "description": "Angular directive to paginate in front end",
  "keywords": [
    "angular",
    "paginate",
    "pagination"
  ],
  "repository": "ovh-ux/ng-pagination-front",
  "license": "BSD-3-Clause",
