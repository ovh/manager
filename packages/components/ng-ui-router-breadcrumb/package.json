{
  "name": "@ovh-ux/ng-ui-router-breadcrumb",
  "version": "1.1.0",
  "description": "Display a breadcrumb based on the UI-Router state declaration.",
  "keywords": [
    "angularjs",
    "breadcrumb",
    "state",
    "ui-router"
  ],
  "homepage": "https://github.com/ovh/manager/tree/master/packages/components/ng-ui-router-breadcrumb#readme",
  "bugs": {
    "url": "https://github.com/ovh/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/components/ng-ui-router-breadcrumb"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ui-router-breadcrumb.js",
  "files": [
    "dist",
    "examples"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ng-ui-router-breadcrumb' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ui-router-breadcrumb' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ui-router-breadcrumb' --include-dependencies -- yarn run dev:watch"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  },
  "peerDependencies": {
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.5.0"
  }
}
