{
  "name": "@ovh-ux/ovh-uapp",
  "version": "0.0.0",
  "description": "OVHcloud micro frontend framework for AngularJS applications",
  "keywords": [],
  "homepage": "https://github.com/ovh/manager/tree/master/packages/components/ovh-uapp#readme",
  "bugs": {
    "url": "https://github.com/ovh/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/components/ovh-uapp"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ovh-uapp.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ovh-uapp' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ovh-uapp' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ovh-uapp' --include-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "postmate": "^1.5.2"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  }
}
