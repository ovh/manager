{
  "name": "@ovh-ux/ng-ovh-contact",
  "version": "5.2.2",
  "description": "Helps you dealing with OVHcloud contacts from /me/contact API.",
  "keywords": [
    "angularjs",
    "contact",
    "ovh",
    "ovhcloud"
  ],
  "homepage": "https://github.com/ovh/manager/tree/master/packages/components/ng-ovh-contact#readme",
  "bugs": {
    "url": "https://github.com/ovh/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/components/ng-ovh-contact"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ovh-contact.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ng-ovh-contact' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ovh-contact' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ovh-contact' --include-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "intl-tel-input": "^17.0.13",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^13.0.1"
  },
  "peerDependencies": {
    "@ovh-ux/ng-ovh-api-wrappers": "^5.0.0",
    "@ovh-ux/ng-translate-async-loader": "^2.1.4",
    "angular": "^1.7.8",
    "angular-translate": "^2.18.1",
    "angular-ui-bootstrap": "~1.3.3",
    "moment": "^2.24.0",
    "ovh-api-services": "^15.0.0",
    "ovh-manager-webfont": "^1.2.0",
    "ui-select": "^0.19.8"
  }
}
