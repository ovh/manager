    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ovh-export-csv' --include-dependencies -- yarn run dev:watch"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^10.0.0 || ^11.0.0"
