{
  "name": "@ovh-ux/ng-ovh-responsive-popover",
  "version": "5.0.5",
  "description": "Module is used to display a popover and adapt it to the screen dimension.",
  "keywords": [
    "angularjs",
    "ovh",
    "ovhcloud",
    "responsive-popover"
  ],
  "homepage": "https://github.com/ovh/manager/tree/master/packages/components/ng-ovh-responsive-popover#readme",
  "bugs": {
