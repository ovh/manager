    "directory": "packages/components/ng-ovh-responsive-popover"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ovh-responsive-popover.js",
  "files": [
    "dist"
  ],
  "scripts": {
