{
  "name": "@ovh-ux/ng-ui-router-layout",
  "version": "3.0.0",
  "description": "UiRouter Layout support",
  "keywords": [
    "angular",
    "layout",
    "ovh",
    "uirouter"
  ],
  "homepage": "https://github.com/ovh-ux/manager/tree/master/packages/components/ng-uirouter-layout#readme",
  "bugs": {
