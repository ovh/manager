    "start": "lerna exec --stream --scope='@ovh-ux/ng-ui-router-layout' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ui-router-layout' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ui-router-layout' --include-filtered-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  },
  "peerDependencies": {
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.5.0",
    "angular-ui-bootstrap": "~1.3.3",
    "ovh-ui-angular": "^3.8.2",
    "ovh-ui-kit": "^2.33.3"
