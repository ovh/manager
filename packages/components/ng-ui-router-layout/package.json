    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ui-router-layout' --include-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  },
  "peerDependencies": {
    "@ovh-ux/ui-kit": "^4.1.12",
