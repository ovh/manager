{
  "name": "@ovh-ux/ng-ui-router-title",
  "version": "3.0.0",
  "description": "AngularJS module for updating browser title/history based on the current ui-router state.",
  "keywords": [
    "angularjs",
    "state",
    "title",
    "ui-router"
  ],
  "homepage": "https://github.com/ovh-ux/manager/tree/master/packages/components/ng-ui-router-breadcrumb#readme",
  "bugs": {
    "url": "https://github.com/ovh-ux/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/components/ng-ui-router-title"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ui-router-title.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ng-ui-router-title' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ui-router-title' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ui-router-title' --include-filtered-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "7.0.0"
  },
  "peerDependencies": {
    "@uirouter/angularjs": "^1.0.0",
    "angular": "^1.5.0"
  }
}