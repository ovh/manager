{
  "name": "@ovh-ux/ng-ovh-actions-menu",
  "version": "5.0.7",
  "description": "An actions menu gives the opportunity to group a set of actions available for a specific context under a single menu",
  "keywords": [
    "actions-menu",
    "angularjs",
    "ovh",
    "ovhcloud"
  ],
  "homepage": "https://github.com/ovh/manager/tree/master/packages/components/ng-ovh-actions-menu#readme",
  "bugs": {
    "url": "https://github.com/ovh/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/components/ng-ovh-actions-menu"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ovh-actions-menu.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ng-ovh-actions-menu' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ovh-actions-menu' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ovh-actions-menu' --include-dependencies -- yarn run dev:watch"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  },
  "peerDependencies": {
    "@ovh-ux/ng-ovh-responsive-popover": "^5.0.2",
    "angular": "^1.7.0",
    "angular-translate": "^2.17.0",
    "jquery": "^2.1.3"
  }
}
