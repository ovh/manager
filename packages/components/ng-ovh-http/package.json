{
  "name": "@ovh-ux/ng-ovh-http",
  "version": "5.0.2",
  "description": "Simple HTTP provider for OVH API.",
  "keywords": [
    "angularjs",
    "ovh",
    "ovhcloud",
    "http",
    "provider"
  ],
  "homepage": "https://github.com/ovh/manager/tree/master/packages/components/ng-ovh-http#readme",
  "bugs": {
    "url": "https://github.com/ovh/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/components/ng-ovh-http"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-ovh-http.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/ng-ovh-http' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/ng-ovh-http' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/ng-ovh-http' --include-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "lodash": "~4.17.15",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^10.0.0 || ^11.0.0"
  },
  "peerDependencies": {
    "angular": "^1.3.x"
  }
}
