{
  "name": "{{modulePackageName}}",
  "version": "0.0.0",
  "private": {{isPrivate}},
  "description": "{{moduleDescription}}",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "{{moduleRepositoryUrl}}"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "sideEffects": false,
  "main": "dist/src/lib.js",
  "types": "dist/types/src/lib.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev:watch": "concurrently \"tsc --watch\" \"tsc-alias --watch\"",
    "lint": "manager-lint --config eslint.config.mjs \"src/**/*.{ts,tsx}\"",
    "lint:fix": "manager-lint --fix --config eslint.config.mjs \"src/**/*.{ts,tsx}\"",
    "prepare": "tsc && tsc-alias",
    "test": "manager-test run",
    "test:coverage": "manager-test run --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@ovh-ux/manager-common-translations": "*",
    "@ovh-ux/manager-config": "^8.0.2",
    "@ovh-ux/manager-core-api": "^0.18.7",
    "@ovh-ux/manager-core-utils": "^0.3.0",
    "@ovh-ux/manager-module-common-api": "*",
    "@ovh-ux/manager-static-analysis-kit": "0.13.0",
    "@ovh-ux/manager-tests-setup": "0.6.1",
    "@ovh-ux/manager-vite-config": "0.15.0",
    "concurrently": "^9.2.1",
    "msw": "2.1.7",
    "tsc-alias": "^1.8.16",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@ovh-ux/manager-common-translations": "*",
    "@ovh-ux/manager-config": "^8.0.2",
    "@ovh-ux/manager-core-api": "^0.18.7",
    "@ovh-ux/manager-core-utils": "^0.3.0",
    "@ovh-ux/manager-module-common-api": "*"
  }
}
