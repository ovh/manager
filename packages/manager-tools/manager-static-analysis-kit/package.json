    "./tsconfig/node-strict": {
      "default": "./dist/tsconfig/node-strict.json"
    },
    "./tsconfig/test-strict": {
      "default": "./dist/tsconfig/test-strict.json"
    }
  },
  "main": "./dist/code-analysis.js",
  "types": "./dist/code-analysis.d.ts",
  "bin": {
    "manager-lint": "./bin/lint-cli.js",
    "manager-perf-budgets": "./bin/perf-budgets-cli.js",
    "manager-perf-budgets-tests": "./bin/perf-budgets-tests-cli.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "tsx scripts/build.ts",
    "postinstall": "tsx scripts/build.ts",
    "lint:modern": "manager-lint --config eslint.config.mts ./src",
    "lint:modern:fix": "manager-lint --fix --config eslint.config.mts ./src",
    "prepare": "tsx scripts/build.ts",
    "test": "echo \"No tests yet.\" && exit 0"
  },
  "dependencies": {
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.29.0",
