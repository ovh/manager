{
  "name": "{{ packageName }}",
  "version": "0.0.0",
  "description": "{{ description }}",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/apps/{{ appName }}"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "tsc && vite",
    "start": "lerna exec --stream --scope='{{ packageName }}' --include-dependencies -- npm run build --if-present",
    "start:dev": "lerna exec --stream --scope='{{ packageName }}' --include-dependencies -- npm run dev --if-present",
    "start:watch": "lerna exec --stream --parallel --scope='{{ packageName }}' --include-dependencies -- npm run dev:watch --if-present",
    "test:e2e": "tsc && npx playwright test --headed",
    "test:e2e:ci": "tsc && npx playwright test",
    "test:e2e:script": "tsc && node ../../../../scripts/run-playwright.js"
  },
  "dependencies": {
    "@ovh-ux/manager-config": "^7.3.0",
    "@ovh-ux/manager-core-api": "*",
    "@ovh-ux/manager-react-core-application": "*",
    "@ovh-ux/manager-core-utils": "*",
    "@ovh-ux/shell": "^3.1.0",
    "@tanstack/react-table": "^8.11.6",
    "@tanstack/react-query": "^5.12.2",
    "@tanstack/react-query-devtools": "^5.13.3",
    "@ovhcloud/ods-common-theming": "^17.1.0",
    "@ovhcloud/ods-components": "^17.1.0",
    "@ovhcloud/ods-theme-blue-jeans": "^17.1.0",
    "@ovh-ux/manager-tailwind-config": "*",
    "@ovhcloud/manager-components": "*",
    "i18next": "^20.4.0",
    "react": "^18.2.0",
    "react-i18next": "^11.18.1",
    "react-router-dom": "^6.3.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@ovh-ux/manager-vite-config": "*",
    "typescript": "^4.3.2",
    "@playwright/test": "^1.34.3",
    "vite": "^4.5.0"
  },
  "regions": [
    "CA",
    "EU",
    "US"
  ]
}
