import React from 'react';
import { OsdsBreadcrumb } from '@ovhcloud/ods-components/breadcrumb/react';
import {
  useBreadcrumb,
  BreadcrumbProps,
} from '@ovh-ux/manager-react-core-application';

function Breadcrumb({ rootLabel }: BreadcrumbProps): JSX.Element {
  const breadcrumbItems = useBreadcrumb({ rootLabel, appName:'{{appName}}' });

 return <OsdsBreadcrumb items={breadcrumbItems} />;
}


export default Breadcrumb;
