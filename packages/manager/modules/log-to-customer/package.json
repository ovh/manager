{
  "name": "@ovh-ux/manager-log-to-customer",
  "version": "2.3.2",
  "description": "Module to handle logs to customer",
  "keywords": [
    "angularjs",
    "api",
    "log",
    "ovh",
    "ovhcloud",
    "poll"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/modules/log-to-customer"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "type": "module",
  "main": "./dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-log-to-customer' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-log-to-customer' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-log-to-customer' --include-dependencies -- yarn run dev:watch"
  },
  "resolutions": {
    "set-value": "^2.0.1"
  },
  "dependencies": {
    "lodash": "~4.17.15"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^13.0.1"
  },
  "peerDependencies": {
    "@ovh-ux/manager-ng-layout-helpers": "^2.8.1",
    "@ovh-ux/ng-log-live-tail": "^2.1.1",
    "@ovh-ux/ng-ovh-swimming-poll": "^5.1.1",
    "@uirouter/angularjs": "^1.0.23",
    "angular": "^1.7.0",
    "angular-translate": "^2.18.1",
    "moment": "^2.24.0"
  }
}
