  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0",
    "bootstrap": "~3.3.7",
    "font-awesome": "^4.0.0",
    "ovh-common-style": "^3.2.2",
    "ovh-ui-kit": "^2.34.3",
    "ovh-ui-kit-bs": "^2.1.2"
  }
