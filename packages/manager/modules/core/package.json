{
  "name": "@ovh-ux/manager-core",
  "version": "9.4.0",
  "private": true,
  "description": "Core functionalities shared across all control panel.",
  "homepage": "https://github.com/ovh/manager/tree/master/packages/manager/modules/core#readme",
  "bugs": {
    "url": "https://github.com/ovh/manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/modules/core"
  },
  "license": "BSD-3-Clause",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/core.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-core' --include-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-core' --include-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-core' --include-dependencies -- yarn run dev:watch"
  },
  "dependencies": {
    "@ovh-ux/ng-translate-async-loader": "^2.1.0",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0"
  },
  "peerDependencies": {
    "@ovh-ux/manager-config": "^0.4.0",
    "@ovh-ux/ng-ovh-http": "^5.0.0",
    "@ovh-ux/ng-ovh-request-tagger": "^1.1.0",
    "@ovh-ux/ng-ovh-sso-auth": "^4.2.3",
    "@ovh-ux/ui-kit": "^4.1.12",
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.5",
    "angular-aria": "^1.7.5",
    "angular-dynamic-locale": "^0.1.37",
    "angular-i18n": "~1.5.x",
    "angular-resource": "^1.7.5",
    "angular-sanitize": "^1.7.5",
    "angular-translate": "^2.18.1",
    "angular-translate-loader-pluggable": "^1.3.1"
  }
}
