  "description": "Sidebar component shared across all control panel.",
  "homepage": "https://github.com/ovh/manager/tree/master/packages/manager/modules/server-sidebar#readme",
  "bugs": {
    "url": "https://github.com/ovh/manager/issues"
  },
  "repository": {
    "type": "git",
