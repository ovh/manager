  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^11.0.0 || ^12.0.0",
    "@ovh-ux/ng-ovh-feature-flipping": "^1.0.0",
    "@ovh-ux/ui-kit": "^6.1.0"
  },
  "peerDependencies": {
    "@ovh-ux/manager-config": "^5.0.0 || ^6.0.0",
    "@ovh-ux/manager-core": "^12.0.0 || ^13.0.0",
    "@ovh-ux/ng-at-internet": "^5.9.1",
