  },
  "peerDependencies": {
    "@ovh-ux/manager-core": "^7.3.0",
    "@ovh-ux/ng-translate-async-loader": "^2.0.0",
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.8",
    "angular-translate": "^2.18.1",
    "ovh-ui-angular": "^3.9.8",
    "ovh-ui-kit": "^2.33.1"
  }
