{
  "name": "@ovh-ux/manager-support",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/support.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -c --environment BUILD:development",
    "dev:watch": "yarn run dev --watch",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-support' --include-filtered-dependencies -- yarn run build",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-support' --include-filtered-dependencies -- yarn run dev",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-support' --include-filtered-dependencies -- yarn run dev:watch"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "5.0.1"
  },
  "peerDependencies": {
    "@ovh-ux/manager-core": "^5.2.0",
    "@ovh-ux/ng-ovh-otrs": "^7.0.2",
    "@ovh-ux/ng-translate-async-loader": "^2.0.0",
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.8",
    "angular-translate": "^2.18.0",
    "bootstrap4": "twbs/bootstrap#v4.0.0",
    "lodash": "^4.17.11",
    "oclazyload": "^1.1.0",
    "ovh-api-services": "^6.18.0",
    "ovh-ui-angular": "^3.2.0",
    "ovh-ui-kit": "^2.33.0",
    "ovh-ui-kit-bs": "^2.1.1"
  }
}
