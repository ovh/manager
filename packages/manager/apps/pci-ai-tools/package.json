    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
