{
  "extends": "@ovh-ux/manager-static-analysis-kit/tsconfig/react-strict",
  "compilerOptions": {
    "types": [
      "vite/client",
      "node",
      "@testing-library/jest-dom"
    ],
    "outDir": "dist",
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "paths": {
      "@secret-manager/*": [ "src/modules/secret-manager/*"],
      "@key-management-service/*": ["src/modules/key-management-service/*"],
      "@/*": ["./src/*"]
    },
    "baseUrl": "."
  },
  "include": ["src", "public/**/*.json"],
  "exclude": ["node_modules", "dist", "types"]
}
