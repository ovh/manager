    "i18next": "^23.8.2",
    "immer": "^10.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.3.0",
    "zustand": "^4.5.5"
