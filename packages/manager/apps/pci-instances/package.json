{
  "name": "@ovh-ux/manager-pci-instances-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
