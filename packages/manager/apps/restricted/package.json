    "bootstrap": "5.0.0",
    "font-awesome": "^4.7.0",
    "i18next": "^23.8.2",
    "i18next-http-backend": "^1.3.0",
    "ovh-manager-webfont": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-responsive": "^9.0.0-beta.6",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@ovh-ux/manager-dev-server-config": "^6.1.1",
    "@types/dompurify": "^2.3.3",
    "@types/react-dom": "^18.0.0",
