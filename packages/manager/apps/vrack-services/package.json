{
  "name": "@ovh-ux/manager-vrack-services-app",
  "version": "0.0.0",
  "description": "OVHcloud VrackServices app",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/apps/vrack-services"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "tsc && vite",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-vrack-services-app' --include-dependencies -- npm run build --if-present",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-vrack-services-app' --include-dependencies -- npm run dev --if-present",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-vrack-services-app' --include-dependencies -- npm run dev:watch --if-present",
    "test:e2e": "tsc && npx playwright test --headed",
    "test:e2e:ci": "tsc && npx playwright test",
    "test:e2e:script": "tsc && node ../../../../scripts/run-playwright.js"
  },
  "dependencies": {
    "@ovh-ux/manager-config": "^7.2.0",
    "@ovh-ux/manager-core-api": "*",
    "@ovh-ux/manager-react-core-application": "*",
    "@ovhcloud/ods-common-core": "^16.0.1",
    "@ovh-ux/shell": "^3.0.0",
    "@ovhcloud/msc-react-tile": "*",
    "@ovhcloud/msc-react-billing-tile": "*",
    "@ovhcloud/ods-components": "^16.0.1",
    "@ovhcloud/ods-common-theming": "^16.0.1",
    "@ovhcloud/ods-theme-blue-jeans": "^16.0.1",
    "@tanstack/react-query": "^4.29.3",
    "@tanstack/react-query-devtools": "^4.9.0",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-i18next": "^11.18.1",
    "react-router-dom": "^6.3.0",
    "i18next": "^20.4.0",
    "tailwindcss": "^3.3.3",
    "msw": "^1.3.2"
  },
  "devDependencies": {
    "@ovh-ux/manager-vite-config": "*",
    "typescript": "^4.3.2",
    "@playwright/test": "^1.34.3",
    "vite": "^3.0.2"
  },
  "peerDependencies": {
    "@ovhcloud/msc-utils": "*"
  },
  "regions": [
    "CA",
    "EU",
    "US"
  ],
  "msw": {
    "workerDirectory": "src/public"
  }
}
