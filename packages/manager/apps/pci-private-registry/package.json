{
  "name": "@ovh-ux/manager-pci-private-registry-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "lint": "eslint ./src",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-pci-private-registry-app' --include-dependencies -- npm run build --if-present",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-pci-private-registry-app' --include-dependencies -- npm run dev --if-present",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-pci-private-registry-app' --include-dependencies -- npm run dev:watch --if-present",
    "test": "vitest run",
    "test:w": "vitest"
  },
  "dependencies": {
    "@ovh-ux/manager-config": "*",
    "@ovh-ux/manager-core-api": "*",
    "@ovh-ux/manager-pci-common": "*",
    "@ovh-ux/manager-react-components": "^1.28.1",
    "@ovh-ux/manager-react-core-application": "*",
    "@ovh-ux/manager-react-shell-client": "*",
    "@ovh-ux/manager-tailwind-config": "*",
    "@ovh-ux/shell": "*",
    "@ovhcloud/ods-common-core": "17.2.2",
    "@ovhcloud/ods-common-stencil": "17.2.2",
    "@ovhcloud/ods-common-theming": "17.2.2",
    "@ovhcloud/ods-components": "17.2.2",
    "@ovhcloud/ods-theme-blue-jeans": "17.2.2",
    "@tanstack/react-query": "^5.52.0",
    "@tanstack/react-query-devtools": "^5.52.0",
    "@tanstack/react-table": "^8.20.1",
    "clsx": "^2.1.1",
    "element-internals-polyfill": "^1.3.11",
    "i18next": "^23.14.0",
    "i18next-http-backend": "^2.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.2",
    "react-i18next": "^15.0.1",
    "react-router-dom": "^6.26.1",
    "react-use": "^17.5.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@ovh-ux/manager-vite-config": "*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.1.1"
  },
  "regions": [
    "CA",
    "EU",
    "US"
  ],
  "universes": [
    "@ovh-ux/manager-public-cloud"
  ]
}
