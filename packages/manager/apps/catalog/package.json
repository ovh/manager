    "i18next-http-backend": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.3.0",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "@ovh-ux/manager-tailwind-config": "^0.2.0",
    "@ovh-ux/manager-vite-config": "^0.8.0",
    "@playwright/test": "^1.41.2",
    "@tanstack/react-query-devtools": "^5.51.21",
    "@types/jest": "^29.5.5",
    "ts-jest": "^29.1.1"
  },
  "regions": [
