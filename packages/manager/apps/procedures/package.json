{
  "name": "@ovh-ux/manager-procedures-app",
  "version": "0.0.0",
  "private": true,
  "description": "Procedures application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/apps/procedures"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "scripts": {
    "build": "tsc && vite build",
    "coverage": "vitest run --coverage",
    "dev": "tsc && vite",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-procedures-app' --include-dependencies -- npm run build --if-present",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-procedures-app' --include-dependencies -- npm run dev --if-present",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-procedures-app' --include-dependencies -- npm run dev:watch --if-present",
    "test": "vitest run",
    "test:e2e": "tsc && npx playwright test --headed",
    "test:e2e:ci": "tsc && npx playwright test",
    "test:e2e:script": "tsc && node ../../../../scripts/run-playwright.js"
  },
  "dependencies": {
    "@ovh-ux/manager-config": "^7.3.0",
    "@ovh-ux/manager-core-api": "^0.7.0",
    "@ovh-ux/manager-tailwind-config": "^0.2.0",
    "@ovhcloud/ods-common-core": "17.2.1",
    "@ovhcloud/ods-common-theming": "17.2.1",
    "@ovhcloud/ods-components": "17.2.1",
    "@ovhcloud/ods-theme-blue-jeans": "17.2.1",
    "@tanstack/react-query": "^5.8.3",
    "@tanstack/react-query-devtools": "^5.8.3",
    "clsx": "^1.2.1",
    "i18next": "^20.4.0",
    "i18next-http-backend": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.4",
    "react-i18next": "^11.18.1",
    "react-router-dom": "^6.3.0",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "@ovh-ux/manager-vite-config": "*",
    "@playwright/test": "^1.34.3",
    "@testing-library/react": "^14.1.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.0",
    "typescript": "^4.3.2",
    "vite": "^4.5.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "axios": "^1.1.2"
  },
  "regions": [
    "CA",
    "EU"
  ]
}
