    "angular-translate-loader-pluggable": "^1.3.1",
    "bootstrap": "~3.3.0",
    "bootstrap-additions": "~0.3.1",
    "bootstrap4": "twbs/bootstrap#v4.0.0",
    "flatpickr": "^4.6.3",
