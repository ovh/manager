    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "postcss": "^8.4.35",
    "rollup": "^4.9.6",
    "tailwindcss": "^3.4.4",
    "vite": "^5.2.13",
    "vitest": "^1.2.0"
