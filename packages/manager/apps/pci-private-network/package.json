    "file-saver": "^2.0.5",
    "nanoid": "^3.1.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.5.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.5"
