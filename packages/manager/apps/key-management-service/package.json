  "scripts": {
    "beta-test:e2e": "tsc && npx playwright test --headed",
    "beta-test:e2e:cii": "tsc && npx playwright test",
    "beta-test:e2e:script": "tsc && node ../../../../scripts/run-playwright.js",
    "build": "tsc && vite build",
    "dev": "tsc && vite",
