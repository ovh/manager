{
  "name": "@ovh-ux/manager-container-app",
  "version": "0.91.3",
  "private": true,
  "description": "OVHcloud Manager Container.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/apps/shell"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "tsc && vite",
    "dev:dedicated": "VITE_CONTAINER_APP=dedicated yarn dev",
    "dev:hub": "VITE_CONTAINER_APP=hub yarn dev",
    "dev:public-cloud": "VITE_CONTAINER_APP=public-cloud yarn dev",
    "dev:telecom": "VITE_CONTAINER_APP=telecom yarn dev",
    "dev:web": "VITE_CONTAINER_APP=web yarn dev",
    "lint": "eslint ./src",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-container-app' --include-dependencies -- npm run build --if-present",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-container-app' --include-dependencies -- npm run dev --if-present",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-container-app' --include-dependencies -- npm run dev:watch --if-present",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
