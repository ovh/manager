{
  "name": "@ovh-ux/manager-container-app",
  "version": "0.4.0",
  "private": true,
  "description": "OVHcloud Manager Container.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/apps/shell"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "tsc && vite",
    "dev:dedicated": "APP=dedicated yarn dev",
    "dev:hub": "APP=hub yarn dev",
    "dev:public-cloud": "APP=public-cloud yarn dev",
    "dev:telecom": "APP=telecom yarn dev",
    "dev:web": "APP=web yarn dev",
    "lint": "eslint ./src",
    "prepare": "yarn run build",
    "start": "lerna exec --stream --scope='@ovh-ux/manager-container-app' --include-dependencies -- npm run build --if-present",
    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-container-app' --include-dependencies -- npm run dev --if-present",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-dedicated' --include-dependencies -- npm run dev:watch --if-present"
  },
  "dependencies": {
    "@ovh-ux/manager-config": "^5.0.0 || ^6.0.0",
    "@ovh-ux/ovh-payment-method": "^0.0.2",
    "@ovh-ux/ovh-reket": "^1.0.0",
    "@ovh-ux/shell": "^0.0.0 || ^1.0.0",
    "@ovh-ux/ui-kit": "^6.1.0",
    "@popperjs/core": "^2.11.4",
    "@tanem/react-nprogress": "^5.0.0",
    "bootstrap": "4.6.0",
    "date-fns": "^2.24.0",
    "dompurify": "^2.3.6",
    "i18next": "^20.4.0",
    "i18next-http-backend": "^1.3.0",
    "lodash-es": "^4.17.15",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.3",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-responsive": "^9.0.0-beta.6",
    "react-router-dom": "^5.3.0",
    "react-select": "^5.3.2",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@ovh-ux/manager-dev-server-config": "^4.0.0 || ^5.0.0",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/dompurify": "^2.3.3",
    "@types/jest": "^28.1.1",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint-plugin-react": "^7.20.3",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.4",
    "msw": "^0.35.0",
    "vite": "^2.2.3"
  },
  "regions": [
    "CA",
    "EU",
    "US"
  ]
}
