    "start:dev": "lerna run dev --stream --scope='@ovh-ux/manager-overthebox-app' --include-filtered-dependencies",
    "start:watch": "lerna run dev:watch --stream --parallel --scope='@ovh-ux/manager-overthebox-app' --include-filtered-dependencies",
