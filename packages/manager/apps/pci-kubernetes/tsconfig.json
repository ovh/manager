{
  "extends": "@ovh-ux/manager-static-analysis-kit/tsconfig/react-strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/public/*": ["./public/*"],
      "@/*": ["./src/*"],
      "react": ["./node_modules/@types/react"]
    },
    "outDir": "dist",
    "allowJs": true,
    "resolveJsonModule": true
  },
  "include": ["src", "public/**/*.json"],
  "exclude": ["node_modules", "dist", "types"]
}
