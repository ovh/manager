    "start:dev": "lerna exec --stream --scope='@ovh-ux/manager-pci-savings-plan-app' --include-dependencies -- npm run dev --if-present",
    "start:watch": "lerna exec --stream --parallel --scope='@ovh-ux/manager-pci-savings-plan-app' --include-dependencies -- npm run dev:watch --if-present",
