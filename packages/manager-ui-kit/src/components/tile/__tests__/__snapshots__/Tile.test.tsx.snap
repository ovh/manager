      data-ods=card
    >
      <section
        class=flex flex-col w-full
      >
        <h4
          class=_text _text--heading-4
          data-ods=text
        >
          Simple Tile
        </h4>
        <hr
          class=_divider _divider--24 w-full
          data-ods=divider
        />
        <dl
          class=flex flex-col m-0
        >
          <div
            class=flex flex-col gap-1
          >
            <dt
              class=flex justify-between
            >
              <div
                class=flex items-center gap-1 text-[var(--ods-color-text)]
              >
                <span
                  class=_text _text--span font-bold
                  data-ods=text
                >
                  Sample Term
                </span>
                <span
                  aria-describedby=tooltip::r0::content
                  class=_icon _icon--circle-question _tooltip-trigger
                  data-expanded=
                  data-ods=tooltip-trigger
                  data-part=trigger
                  data-scope=tooltip
                  data-state=open
                  dir=ltr
                  id=tooltip::r0::trigger
                  role=button
                  tabindex=0
                />
              </div>
